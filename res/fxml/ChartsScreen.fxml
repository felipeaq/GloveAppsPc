<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="1000.0" prefWidth="1000" style="-fx-background-color: #dcdcdc;" stylesheets="@css/css.css" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.ChartsScreenController">
   <children>
      <VBox fx:id="accGraphsVbox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
      <VBox fx:id="gyroGraphsVbox" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
       <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefWidth="1000.0" VBox.vgrow="NEVER">
           <JFXButton mnemonicParsing="false" onAction="#start" prefHeight="55.0" prefWidth="150.0" styleClass="keyboard-left-buttons" text="Start">
               <font>
                   <Font size="20.0" />
               </font>
               <HBox.margin>
                   <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
           </JFXButton>
           <JFXButton layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#stop" prefHeight="55.0" prefWidth="150.0" styleClass="keyboard-left-buttons" text="Stop">
               <font>
                   <Font size="20.0" />
               </font>
               <HBox.margin>
                   <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
           </JFXButton>
       </HBox>
   </children>
</VBox>
